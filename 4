#include <iostream>
#include <cmath>
using namespace std;
int main()
{
	float p = -1, pmax = -1;
	int i, K=0, N, M, min, c = 0, j, c1=0, s = 0; 
	float m;
	cin >> N>>M;
	int *A[N];
	for	(i=0; i<M; i++) A[i] = new int[N];
	cout << "Vvedite elementy matricy: ";
	for (i = 0; i < M; i++){
		for (j = 0; j < N; j++)
			cin >> A[i][j];
	}
	
	for (i=0; i<M; i++){
		m=0;
		K=0;
	for (j = 0; j < N; j++){
		 m+=A[j][i];
	}
	m/=j;
	for (j = 0; j < N; j++){
		 if (A[j][i]<m) K++;
	}
	cout<<"v "<<i+1<<" stolbce "<<K<<" elementov < sr arif"<<endl;
}
}
